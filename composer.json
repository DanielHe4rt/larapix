{
  "name": "danielhe4rt/larapix",
  "description": "OpenPix Laravel SDK",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "danielhe4rt",
      "email": "hey@danielheart.dev"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "6.5.5",
    "illuminate/support": "~5.3.0",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "ext-curl": "*",
    "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Liuv\\Larapix\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Liuv\\Tests\\": "tests/"
    }
  }
}
