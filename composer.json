{
  "name": "danielhe4rt/larapix",
  "description": "OpenPix Laravel SDK",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "danielhe4rt",
      "email": "hey@danielheart.dev"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.7",
    "illuminate/support": "^10.0"
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "ext-curl": "*",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "DanielHe4rt\\Larapix\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DanielHe4rt\\Larapix\\Tests\\": "tests/"
    }
  }
}
